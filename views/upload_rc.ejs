        <%- include ("nav.ejs") %>

 <div class="container">
    <div class="col-md-12">
      <div class="panel with-nav-tabs panel-primary">
            <div class="tab-pane fade" id="Tabs-Ketiga">
              <div class="col-md-12">
                <!-- untuk backend actionnya ditunjukan ke mana -->
                <form action="/upload" method="post" enctype="multipart/form-data">
                  <div class="form-group files">
                    <label>Upload Your Registration documents </label>
                    <input type="file" name="pic" accept="image/*">
                  </div>
                  </div>
                </form>
              </div>
            </div>

            <div class="tab-pane fade in active" id="Tabs-Keempat">
              <!-- disini maksudnya in active yaitu mengaktifkan file yg ditampilkan untuk pertamakali -->
              <div class="col-md-12">
                    <form action="/upload" method="post" enctype="multipart/form-data">
                  <div class="form-group files color">
                    <label>Upload Your Registration documents </label>
                    <input type="file" name="pic" accept="image/*">
                  </div>
                  <div class="col-md-12">
                    <button type="submit" class="btn btn-labeled btn-success pull-right">
                      <span class="btn-label"><i class="glyphicon glyphicon-ok"></i></span>Save
                    </button>
                  </div>
                </form>
              </div>
            </div>

          </div>
        </div>

      </div>
</body>
</html>